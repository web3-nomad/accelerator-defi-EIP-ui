{
  "extends": ["next/core-web-vitals", "prettier"],
  "plugins": ["prettier","react-hooks"],
  "rules": {
    "react-hooks/exhaustive-deps": [
      "warn",
      {
        "enableDangerousAutofixThisMayCauseInfiniteLoops": true
      }
    ],
    "prettier/prettier": [
      "warn",
      {
        "endOfLine": "auto"
      }
    ]
  }
}
